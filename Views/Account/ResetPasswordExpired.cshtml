@{
    ViewData["Title"] = "Reset Link Expired";
}

<div class="text-center py-5">
    <h2 style="color: red;">Reset Link Expired or Invalid</h2>
    <p>The password reset link has expired or has already been used.</p>
    <a asp-action="ForgotPassword" asp-controller="Account" class="btn btn-warning">
        Request New Link
    </a>
</div>